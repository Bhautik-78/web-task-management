    "@types/node": "14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/uikit": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
