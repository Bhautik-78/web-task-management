  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "test": "jest",
    "test:reporter": "yarn jest --ci --testResultsProcessor='./node_modules/jest-junit-reporter'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:write": "prettier . --write",
    "start": " parcel src/index.html -p 3000",
    "build-lib": "cd lib/jera-design-ui && yarn",
    "link:jera-design-ui": "cd lib/jera-design-ui && yarn link && cd ../../ && yarn link jera-design-ui",
